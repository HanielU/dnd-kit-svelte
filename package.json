{
	"name": "svelte-dnd-kit",
	"version": "0.0.2",
	"license": "MIT",
	"repository": "github:HanielU/svelte-dnd-kit",
	"author": "Haniel Ubogu <https://github.com/HanielU>",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./core": {
			"types": "./dist/core/src/index.d.ts",
			"svelte": "./dist/core/src/index.js"
		},
		"./utilities": {
			"types": "./dist/utilities/src/index.d.ts",
			"svelte": "./dist/utilities/src/index.js"
		},
		"./sortable": {
			"types": "./dist/sortable/src/index.d.ts",
			"svelte": "./dist/sortable/src/index.js"
		},
		"./modifiers": {
			"types": "./dist/modifiers/src/index.d.ts",
			"svelte": "./dist/modifiers/src/index.js"
		},
		"./accessibility": {
			"types": "./dist/accessibility/src/index.d.ts",
			"svelte": "./dist/accessibility/src/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@iconify/json": "^2.2.293",
		"@sveltejs/adapter-vercel": "^5.5.2",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/node": "^22.10.5",
		"@unocss/preset-mini": "^0.65.4",
		"@unocss/reset": "^0.65.4",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"unocss": "^0.65.4",
		"unocss-preset-animations": "^1.1.0",
		"unplugin-icons": "^22.0.0",
		"vite": "^5.4.11",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"esm-env": "^1.2.2",
		"runed": "^0.23.0",
		"svelte-toolbelt": "^0.7.0"
	}
}
